<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<BorderPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox prefHeight="50.0" prefWidth="750.0" spacing="10.0" style="-fx-background-color: #336699;" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" prefHeight="20.0" prefWidth="100.0" text="Current" />
            <Button mnemonicParsing="false" prefHeight="20.0" prefWidth="100.0" text="Rejected" />
         </children>
         <padding>
            <Insets bottom="15.0" left="12.0" right="12.0" top="15.0" />
         </padding>
      </HBox>
   </top>
   <left>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="8.0" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Data">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
            </Text>
            <Hyperlink text="Sales">
               <VBox.margin>
                  <Insets left="8.0" />
               </VBox.margin>
            </Hyperlink>
            <Hyperlink text="Marketing">
               <VBox.margin>
                  <Insets left="8.0" />
               </VBox.margin>
            </Hyperlink>
            <Hyperlink text="Distribution">
               <VBox.margin>
                  <Insets left="8.0" />
               </VBox.margin>
            </Hyperlink>
            <Hyperlink text="Costs">
               <VBox.margin>
                  <Insets left="8.0" />
               </VBox.margin>
            </Hyperlink>
         </children>
      </VBox>
   </left>
</BorderPane>
